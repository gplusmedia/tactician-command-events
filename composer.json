{
    "name": "league/tactician-command-events",
    "description": "Tactician Command Events",
    "license": "MIT",
    "keywords": ["event", "tactician"],
    "homepage": "http://tactician.thephpleague.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "league/tactician": "^1.0",
        "league/event": "~2.1"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0"
    },
    "autoload": {
        "psr-4": {
            "League\\Tactician\\CommandEvents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Tactician\\CommandEvents\\Stub\\": "stubs/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.6-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
