{
    "name": "league/tactician-command-events",
    "description": "Tactician Command Events",
    "license": "MIT",
    "keywords": ["event", "tactician"],
    "homepage": "http://tactician.thephpleague.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "league/tactician": "dev-master",
        "league/event": "~2.0.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1.0",
        "henrikbjorn/phpspec-code-coverage" : "~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "League\\Tactician\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Tactician\\Stub\\": "stub/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.0-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
