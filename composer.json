{
    "name": "league/tactician-command-events",
    "description": "Tactician Command Events",
    "license": "MIT",
    "keywords": ["event", "tactician"],
    "homepage": "http://tactician.thephpleague.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "league/tactician": "^1.1",
        "league/event": "^3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "League\\Tactician\\CommandEvents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\League\\Tactician\\CommandEvents\\": "spec/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpspec run",
        "test-ci": "vendor/bin/phpspec run -c phpspec.ci.yml"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.6-dev"
        }
    }
}
